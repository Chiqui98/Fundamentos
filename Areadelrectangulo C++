#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;
void main()
{
	float base,altura,area;
	cout<<"Ingrasar base"<<endl;
	cin>>base;
	cout<<"ingrese altura"<<endl;
	cin>>altura;
	area=base*altura;
	cout<<"Area del rectangulo : "<<area;

    getch();
}
